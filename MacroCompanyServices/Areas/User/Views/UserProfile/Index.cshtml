@{
    string strTitle = "User information";
    ViewBag.Title = strTitle;
}

<div>
    <h1>@strTitle</h1>
    <div>
        <p>User ID: @ViewBag.userId</p>
        <p>Username: @ViewBag.userName</p>
    </div>
    <div>
        <a asp-area="User" asp-controller="UserProfile" asp-action="Edit" asp-route-id="@ViewBag.userId">Edit profile</a>
    </div>
    <div>
        <form asp-area="" asp-controller="Account" asp-action="Logout" method="post">
            <input type="submit" value="Logout" />
        </form>
    </div>
</div>